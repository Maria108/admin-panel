<h1>Listing cohorts</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
  </tr>
 
  <% @cohorts.each do |cohort| %>
    <tr>
      <td><%= cohort.name %></td>
      <td><%= cohort.start_date %></td>
      <td><%= cohort.end_date %></td>
      <td><%= link_to 'Show', cohort_path(cohort) %></td>
    </tr>
  <% end %>
</table>
